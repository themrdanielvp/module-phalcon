{
    "name":"themrdanielvp/module-phalcon",
    "description":"Codeception module for Phalcon framework",
    "keywords":["codeception", "phalcon"],
    "homepage":"https://codeception.com/",
    "type":"library",
    "license":"MIT",
    "authors":[
        {
            "name":"Serghei Iakovlev"
        }
    ],
    "minimum-stability": "RC",
    "require": {
        "php": ">=5.6.0 <8.2",
        "codeception/lib-innerbrowser": "^3.1.2",
        "codeception/codeception": "^5.0"
    },
    "require-dev": {
        "codeception/module-asserts": "^1.0",
        "codeception/module-phpbrowser": "^1.0"
    },
    "autoload":{
        "classmap": ["src/"]
    },
    "config": {
        "classmap-authoritative": true
    }
}
